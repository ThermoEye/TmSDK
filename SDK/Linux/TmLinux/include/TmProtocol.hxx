    class TmProtocol
    {
    public:
        static uint32_t BuildPacket(Packet pkt, uint8_t*& pktData);
        static bool ParsePacket(std::vector<uint8_t> pktData, Packet& packet);
        
        template<typename T>
        static T ByteToStruct(const std::vector<unsigned char>& buffer) {
            if (buffer.size() < sizeof(T)) {
                throw std::runtime_error("Buffer size is smaller than the structure size.");
            }

            T obj;
            memcpy(&obj, buffer.data(), sizeof(T));
            return obj;
        }

        template<typename T>
        static std::vector<uint8_t> StructToByte(const T& obj) {
            std::vector<uint8_t> arr(sizeof(T));
            memcpy(arr.data(), &obj, sizeof(T));
            return arr;
        }
    };

