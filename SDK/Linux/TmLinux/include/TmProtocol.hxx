    class Parameter
    {
    public:
        uint16_t paramName;
        uint16_t paramValue;

        Parameter(uint8_t name, uint16_t value) : paramName(name), paramValue(value) {}
        std::vector<uint8_t> GetBytes()
        {
            std::vector<uint8_t> result;
            result.push_back(static_cast<uint8_t>(paramName & 0xFF));
            result.push_back(static_cast<uint8_t>((paramName >> 8) & 0xFF));
            result.push_back(static_cast<uint8_t>(paramValue & 0xFF));        // 하위 바이트
            result.push_back(static_cast<uint8_t>((paramValue >> 8) & 0xFF)); // 상위 바이트
            return result;
        }
    };

    /// @cond DOXYGEN_SHOULD_SKIP_THIS
    class Packet
    {
    public:
        uint32_t command;
        std::vector<uint8_t> payload;
        uint8_t checksum;
            
        Packet() : command(0), checksum(0) {}
        Packet(uint32_t cmd, const std::vector<uint8_t>& data) : command(cmd), payload(data), checksum(0) {}
            
        void IntToBytes(int value, unsigned char bytes[4]) const {
            bytes[3] = (value >> 24) & 0xFF;
            bytes[2] = (value >> 16) & 0xFF;
            bytes[1] = (value >> 8) & 0xFF;
            bytes[0] = value & 0xFF;
        }
        uint16_t ToUInt16()
        {
            uint16_t val;
            memcpy(&val, payload.data(), sizeof(val));
            return val;
        }
        uint32_t ToUInt32()
        {
            uint32_t val;
            memcpy(&val, payload.data(), sizeof(val));
            return val;
        }
    };
